{
  "singleQuote": true,
  "semi": false,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/(.*)$",
    "^@/lib/(.*)$",
    "^@/components/(.*)$",
    "^@/ui/(.*)$",
    "^@/assets/(.*)$",
    "^[./]"
  ]
}